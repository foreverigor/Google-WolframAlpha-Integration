{
	"manifest_version": 2,

	"name": "Google WolframAlpha Integration",
	"description": "ToDo",
	"version": "1.0",

	"_comment_page_action" : "Icon in address bar - doesn't work yet",
	"page_action": {
	  "default_icon": "icon.png",
	  "default_title": "Google WolframAlpha Integration",
	  "default_popup": "popup.html"
	},
	
 "content_scripts": [
    {
      "matches": ["*://*.google.de/search*", "*://*.google.com/search*"],
      "css": ["inject.css"],
      "js": ["js/jquery.js", "js/inject.js"]
    }
    ],
   "permissions": ["http://*.www.maxi-muth.de"]   
}
