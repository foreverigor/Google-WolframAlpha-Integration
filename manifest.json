{
	"manifest_version": 2,

	"name": "Google WolframAlpha Integration",
	"description": "ToDo",
	"version": "1.0",

	"page_action": {
	  "default_icon": "icon.png",
	  "default_title": "Google WolframAlpha Integration",
	  "default_popup": "popup.html"
	},
	
 "content_scripts": [
    {
      "matches": ["*://*.google.de/search*", "*://*.google.com/search*"],
      "css": ["inject.css"],
      "js": ["js/jquery.js", "js/inject.js"]
    }
    ],
	
 "permissions": ["tabs", "http://api.wolframalpha.com/v2/query?*", "http://*.maxi-muth.de/*"]
}
